#include <stdio.h>

int main()
{
  int x, y, z;
    printf("Ingrese un valor para x ");
    scanf("%d", &x);
    printf("\n Ingrese un valor para y ");
    scanf("%d", &y);

    // Swap de los valores
    z = x;
    x = y;
    y = z;
    
    printf("\nDespues del swap: x = %d, y = %d", x, y);
    return 0;
}
